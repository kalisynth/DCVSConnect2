<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.nac.kalisynth.dcvsconnect2.DCVSHelp"
    android:background="@drawable/dcvs_ui_help_screen"
    tools:ignore="Overdraw"
    android:contentDescription="@string/SkypeCheck">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/buttonhelpguide"
        android:contentDescription="@string/HelpGuideBTN"
        android:onClick="helpguideonclick"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageButton" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SkypeVersion"
        android:background="@drawable/buttonskypecheck"
        android:onClick="skypeversionclick"
        android:contentDescription="@string/SkypeCheck"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="gotoassist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="fastsupportclick"
        android:id="@+id/gtabtn"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="177dp"
        android:layout_marginTop="55dp" />

</RelativeLayout>
